generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model apps {
  id                     String  @id @default(auto()) @map("_id") @db.ObjectId
  Applied_On             String? @map("Applied On")
  Behaviourial_Interview Boolean @map("Behaviourial Interview")
  Company                String
  Connection_to_Company  String? @map("Connection to Company")
  Design_Related         Boolean @map("Design Related")
  Email_Questions        Boolean @map("Email Questions")
  Job_Title              String  @map("Job Title")
  One_Sided_Interview    Boolean @map("One-Sided Interview")
  Portfolio_Walkthrough  Boolean @map("Portfolio Walkthrough")
  Private_Posting        Boolean @map("Private Posting")
  Recruiter_Call         Boolean @map("Recruiter Call")
  Referred               Boolean
  Status                 String
  Tailored_App           Boolean @map("Tailored App")
  Take_home_Challenge    Boolean @map("Take-home Challenge")
  Year                   Int
}
